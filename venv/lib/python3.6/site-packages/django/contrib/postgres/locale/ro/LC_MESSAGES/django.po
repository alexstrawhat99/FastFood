#: venv/lib/python3.6/site-packages/django/contrib/postgres/apps.py:13
msgid "PostgreSQL extensions"
msgstr "Extensiile PostgreSQL"

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/array.py:22
#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/array.py:16
#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Elementul %(nth)s din mulțime nu s-a validat:"

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/array.py:23
msgid "Nested arrays must have the same length."
msgstr "Vectorii imbricați trebuie să aibă aceeași lungime."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "Asociere de șiruri de caractere cu șiruri de caractere/null."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/hstore.py:18
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "Valoarea lui \"%(key)s\" nu este un șir de caractere sau null."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "A JSON object"
msgstr "Un obiect JSON"

#: venv/lib/python3.6/site-packages/django/contrib/postgres/fields/jsonb.py:34
msgid "Value must be valid JSON."
msgstr "Valoarea trebuie să fie validă JSON."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "Nu am putut încărca datele JSON."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Intrarea trebuie să fie un dicționar JSON valid."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "Valorile '%(value)s' trebuie să fie valide JSON."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:16
msgid "Enter two valid values."
msgstr "Introdu două valori valide."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:17
msgid "The start of the range must not exceed the end of the range."
msgstr ""
"Începutul intervalului nu trebuie să depășească sfârșitul intervalului."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:64
msgid "Enter two whole numbers."
msgstr "Introdu două numere întregi."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:70
msgid "Enter two numbers."
msgstr "Introdu două numere."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:76
msgid "Enter two valid date/times."
msgstr "Introdu două date / ore valide."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two valid dates."
msgstr "Introdu două date valide."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai mult de "
"%(limit_value)d."
msgstr[1] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai mult de "
"%(limit_value)d."
msgstr[2] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai mult de "
"%(limit_value)d."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai puțin de "
"%(limit_value)d."
msgstr[1] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai puțin de "
"%(limit_value)d."
msgstr[2] ""
"Lista conține %(show_value)d, nu ar trebui să conțină mai puțin de "
"%(limit_value)d."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Unele chei lipsesc: %(keys)s"

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Au fost furnizate chei necunoscute: %(keys)s"

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Asigură-te că intervalul este în întregime mai mic sau egal cu  "
"%(limit_value)s."

#: venv/lib/python3.6/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Asigură-te că intervalul este în întregime mai mare sau egal cu "
"%(limit_value)s."
